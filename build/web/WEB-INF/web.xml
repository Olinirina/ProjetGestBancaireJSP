<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AjoutClient</servlet-name>
        <servlet-class>Client.AjoutClient</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ModifierClient</servlet-name>
        <servlet-class>Client.ModifierClient</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AjoutVersement</servlet-name>
        <servlet-class>Versement.AjoutVersement</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ModifierVersement</servlet-name>
        <servlet-class>Versement.ModifierVersement</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AjoutRetrait</servlet-name>
        <servlet-class>Retrait.AjoutRetrait</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ModifierRetrait</servlet-name>
        <servlet-class>Retrait.ModifierRetrait</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AjoutVirement</servlet-name>
        <servlet-class>Virement.AjoutVirement</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ModifierVirement</servlet-name>
        <servlet-class>Virement.ModifierVirement</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AjoutClient</servlet-name>
        <url-pattern>/AjoutClient</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ModifierClient</servlet-name>
        <url-pattern>/ModifierClient</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AjoutVersement</servlet-name>
        <url-pattern>/AjoutVersement</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ModifierVersement</servlet-name>
        <url-pattern>/ModifierVersement</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AjoutRetrait</servlet-name>
        <url-pattern>/AjoutRetrait</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ModifierRetrait</servlet-name>
        <url-pattern>/ModifierRetrait</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AjoutVirement</servlet-name>
        <url-pattern>/AjoutVirement</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ModifierVirement</servlet-name>
        <url-pattern>/ModifierVirement</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
